#filters
  bottom -135px
  left 0
  position fixed
  width 100%
  height 185px
  padding 20px 0
  z-index 128
  background-color rgb(249, 249, 249)
  text-align center
  >div
  	display block
  	height 145px
  	border-left 1px solid #ccc
  	&:first-child
  		border none
  #filterHandle
  	width 100%
  	position absolute
  	top -20px
  	height 20px
  	text-align center
  	.text
	    color rgb(61, 61, 61)
	    font-size 0.75em
	    margin 3px auto 0
	    text-align center
	    text-transform uppercase
	    width 38px
	    margin 0 auto
	    background-color: rgb(249, 249, 249);
	    border-top-left-radius: 5px;
	    border-top-right-radius: 5px;
	    display block
	    height 20
	    padding-top 5px
	    width 75px
  input[type="radio"]
    display none
  .filter-button
	  background-color rgba(255, 255, 255, 0)
	  border 4px solid #ffffff
	  border-radius 8px
	  color rgb(87, 87, 87)
	  display table
	  font-family "pt-sans","source sans pro"
	  font-weight 600
	  text-align center
	  text-transform uppercase
	  margin 0 auto
	  width 60%
	  &.gender
	  	height 55px
	  	width 60%
	  	margin-bottom 15px
	  &.type,
	  &.order
	  	height 34px
	  	margin 0 15px 11px
	  	width 80%
	  &:hover,
	  &.active
	    background-color white
	    border 4px solid #b4b4b4
	    box-shadow 0 5px 0 #dadada
	    color rgb(87, 87, 87)
	.colour
		text-indent -99999px
		height 25px
		width 25px
		border-radius 50%;
		margin 10px 0
	.goexplore
	  font-size 14px
	  background-color rgb(54, 54, 54)
	  border 4px solid #b4b4b4
	  border-radius 8px
	  box-shadow 0 5px 0 #dadada
	  color rgb(255, 255, 255)
	  font-family "pt-sans","source sans pro"
	  font-weight 600
	  height 91px
	  width: 60%;
	  margin: 0 auto
	  text-align center
	  text-transform uppercase
	  display table
	.child
		display: table-cell;
		vertical-align: middle;
	.prefer
	  color rgb(133, 133, 133)
	  font italic 400 0.75em/1.38 Helvetica
	  text-align center
	  text-decoration underline
	  margin-top 20px

